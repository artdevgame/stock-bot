    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "parcel": "2.0.0-nightly.537",
    "prettier": "2.2.1",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3"
